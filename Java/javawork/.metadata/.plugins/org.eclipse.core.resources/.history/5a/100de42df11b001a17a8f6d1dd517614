package workbook.section5;

public class Student {
	private String name;
	private int age;
	private int height;
	private int weight;
	
	public Student() { }

	public Student(String name, int age, int height, int weight) {
		this.name = name;
		this.age = age;
		this.height = height;
		this.weight = weight;
	}
	
	
	

}
