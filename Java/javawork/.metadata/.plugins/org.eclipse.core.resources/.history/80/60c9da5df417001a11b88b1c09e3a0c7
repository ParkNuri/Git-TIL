package workbook.section2;

public class Test01 {
	public static void main(String[] args) {
		int inx = 15;
		if(10<inx&&inx<20) {
			System.out.println("true");
		}
		
		char ch1 = ' ';
		if(ch1!=' '||ch1!='\t') {System.out.println("true");}
		
		char ch2 = 'x';
		if(ch2=='x'||ch2=='X') {System.out.println("true");}
		char ch3 = '0';
		if() {System.out.println("true");}
		char ch4 = 'a';
		if() {System.out.println("true");}
		int year = 400;
		if() {System.out.println("true");}
		boolean powerOn = false;
		if() {System.out.println("true");}
		String str = "yes";
		if() {System.out.println("true");}
		
	}
}
