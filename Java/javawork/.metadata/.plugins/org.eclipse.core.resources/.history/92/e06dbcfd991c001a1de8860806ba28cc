package chap07_oop;
/*
 *	<<상속관계에서 생성자가 갖는 특징>> 
 * 	1. 클래스의 모든 생성자 메소드의 첫 번째 문장에는 부모클래스의 기본생성자를 호출하는 명령문이 생략되어 있다.
 * 
 * 
 * 
 * 
 */

class SuperA{
	String name;
	int age;
	public SuperA(String name, int age) {
		super();
		this.name = name;
		this.age = age;
	}
}
class SubB extends SuperA{
	String addr;
}


public class InheritanceTest03 {

	public static void main(String[] args) {

	}

}
