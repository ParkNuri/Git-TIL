package workbook.section2;

public class Test06 {

	public static void main(String[] args) {
		int i=1;
		while(true) {
			for(int j =2;j<10;j++) {
				System.out.print(j+"*"+i+"="+j*i+"\t");
				
				if(i==4) {i=1;}
				if(j%3==1) {
					System.out.println();
					if(i<4) {
						j=j-2;
					}
				}
				i++;
			}
			break;
			
		}
	}
}
