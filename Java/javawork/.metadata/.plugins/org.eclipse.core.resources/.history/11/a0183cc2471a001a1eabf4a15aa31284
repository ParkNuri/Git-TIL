package chap05;
//2차원 배열의 선언과 생성
public class Array2DTest02 {

	public static void main(String[] args) {
		int[][] myarr= new int[3][];
		myarr[0]=new int[3];
		myarr[1]=new int[2];
		myarr[2]=new int[1];
	
		for(int outer=0;outer<3;outer++) {
			for(int i=0;i<myarr[outer].length;i++) {
				System.out.print(myarr[outer][i]+"\t");
			}
			System.out.println();
		}
	}

}
